    "@codemirror/lang-javascript": "^0.20.0",
    "@codemirror/theme-one-dark": "^0.20.0",
    "@icon-park/vue-next": "^1.3.6",
    "@playwright/test": "^1.21.1",
    "@vitejs/plugin-vue": "^2.3.2",
    "electron": "18.2.0",
    "electron-builder": "^23.0.3",
    "electron-debug": "^3.2.0",
