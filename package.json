{
  "name": "sql-mock",
  "version": "1.0.0",
  "description": "通过Mock.js和自定义脚本生成数据库数据，支持导出Work和Markdown文档",
  "main": "./out/main/index.js",
  "author": "Baymax",
  "homepage": ".",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@types/mockjs": "^1.0.7",
    "dayjs": "^1.11.6",
    "docxtemplater": "^3.32.4",
    "file-saver": "^2.0.5",
    "jszip-utils": "^0.1.0",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pizzip": "^3.1.3",
    "sequelize": "^6.25.8",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.11.3"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.39.1",
    "@arco-plugins/vite-vue": "^1.4.2",
    "@arco-themes/vue-db-mock": "^0.0.1",
    "@arco-themes/vue-mgo-blog": "0.0.2",
    "@codemirror/lang-javascript": "^6.1.1",
    "@codemirror/state": "^6.1.4",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/node": "16.11.22",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "codemirror": "^6.0.1",
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.26",
    "pinia-plugin-persistedstate": "^1.6.3",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue": "^3.2.45",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  }
}
